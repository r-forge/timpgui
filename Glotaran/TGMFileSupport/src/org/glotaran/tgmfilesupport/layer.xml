                </folder>
                <folder name="Factories">
                    <file name="TgmDataLoader.instance">
                        <attr name="dataObjectClass" stringvalue="org.glotaran.tgmfilesupport.TgmDataObject"/>
                        <attr name="instanceCreate" methodvalue="org.openide.loaders.DataLoaderPool.factory"/>
                        <attr name="mimeType" stringvalue="text/tgm+xml"/>
                    </file>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="Services">
        <folder name="MIMEResolver">
            <file name="TgmResolver.xml" url="TgmResolver.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.glotaran.tgmfilesupport.Bundle"/>
            </file>
        </folder>
    </folder>
    <folder name="Templates">
        <folder name="Glotaran">
            <file name="TgmTemplate.xml" url="TgmTemplate.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.glotaran.tgmfilesupport.Bundle"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateWizardURL" urlvalue="nbresloc:/org/glotaran/tgmfilesupport/Description.html"/>
            </file>
        </folder>
    </folder>
</filesystem>
