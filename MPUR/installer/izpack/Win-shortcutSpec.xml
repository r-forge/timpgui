<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>
    <skipIfNotSupported/>
    <programGroup defaultName="TIMPGUI $APP-VERSION" location="applications"/>

    <shortcut
        name="TIMPGUI $APP-VERSION"
        programGroup="yes"
        desktop="yes"
        applications="no"
        startMenu="no"
        startup="no"
        target="$INSTALL_PATH\bin\timpgui.exe"
        commandLine=""
        description="TIMPGUI launcher"
        iconFile="$INSTALL_PATH\timpgui\timpgui.ico"
        iconIndex="0"
        initialState="noShow">
    </shortcut>

    <shortcut
        name="Readme.html"
        programGroup="yes"
        desktop="no"
        applications="no"
        startMenu="no"
        startup="no"
        target="$INSTALL_PATH\readme.html"
        commandLine=""
        description="TIMPGUI Readme">
    </shortcut>

    <shortcut
        name="Uninstaller"
        programGroup="yes"
        desktop="no"
        applications="no"
        startMenu="no"
        startup="no"
        target="$INSTALL_PATH\Uninstaller\uninstaller.jar"
        commandLine=""
        description="TIMPGUI uninstaller">
    </shortcut>

</shortcuts>